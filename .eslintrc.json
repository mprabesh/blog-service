{
  "env": {
    "node":true,
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console":"warn",
    "quotes": [1, "double",{"allowTemplateLiterals":true}],
    "eqeqeq":"warn",
    "no-var":"error"
  }
}
